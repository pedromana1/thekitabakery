{%- liquid
    assign social_links = false
    assign localization_forms = false
    assign local_in_topbar = false
     assign has_topbar = false 

    if settings.social_twitter_link != blank or settings.social_facebook_link != blank or settings.social_pinterest_link != blank or settings.social_instagram_link != blank or settings.social_youtube_link != blank or settings.social_vimeo_link != blank or settings.social_tiktok_link != blank or settings.social_tumblr_link != blank or settings.social_snapchat_link != blank
      assign social_links = true
    endif

    if localization.available_countries.size > 1 and section.settings.enable_country_selector or section.settings.enable_language_selector and localization.available_languages.size > 1
      assign localization_forms = true
    endif

    assign topbar = section.blocks | where: 'type', 'topbar'

    if topbar.size == 1 
       assign has_topbar = true 
    endif

    for block in section.blocks
      if block.type == '@app'
        assign has_app_block = true
      endif
    endfor 

    assign color_scheme = section.settings.color_scheme
    assign header_transparent = false  

    if section.settings.enable_transparent and template == 'index'
      assign header_transparent = true 
    endif

    if has_topbar and localization_forms and section.settings.local_selector_topbar != blank 
       assign local_in_topbar = true 
    endif
-%}

{%- capture color_sub_scheme -%}
      color-{{ section.settings.sub_color_scheme }}  
{%- endcapture -%}

{% if header_transparent and request.page_type == 'index' %}
  {% style %}
 
      .header-wrapper--transparent {
        margin-bottom: calc(-1 * var(--header-height, 0px) + var(--topbar-height, 0px)); 
        position: sticky; 
        background-color: transparent; 
          --color-transparent-foreground: {{ section.settings.transparent_text.red }},{{ section.settings.transparent_text.green }},{{ section.settings.transparent_text.blue }};
        {% if section.settings.hover_background_gradient != empty %}
          --gradient-hover-background: {{ section.settings.hover_background_gradient }};
        {% else %}
          --gradient-hover-background: {{ section.settings.hover_background }};
        {% endif %}
        --color-hover-foreground: {{ section.settings.hover_text.red }},{{ section.settings.hover_text.green }},{{ section.settings.hover_text.blue }};
        {% if section.settings.transparent_hover_logo == 'inverse' %}
          --logo-hover-transparent: var(--logo-inverse-background-image);
          {% else %}
          --logo-hover-transparent: var(--logo-background-image);
        {% endif %} 
         {% if section.settings.transparent_logo == 'inverse' %}
          --logo-transparent: var(--logo-inverse-background-image);
          {% else %}
          --logo-transparent: var(--logo-background-image); 
        {% endif %}
      }

  {% endstyle %}
{% endif %}

<{% if section.settings.sticky_header_type != 'none' %}sticky-header data-sticky-type="{{ section.settings.sticky_header_type }}"{% else %}div{% endif %} class="header-wrapper{% if header_transparent %} header-wrapper--transparent{% endif %}{% if section.settings.enable_inverse_logo %} logo--inverse{% endif %}{% unless header_transparent %} color-{{ color_scheme }}{% endunless%} gradient{% if section.settings.show_line_separator %} header-wrapper--border-bottom{% endif %}">
  
  {%- liquid
   if has_topbar 
    for block in topbar
      render 'header-topbar', block: block, section: section, color_sub_scheme: section.settings.topbar_color_scheme, localization_forms: localization_forms, social_links: social_links, local_in_topbar: local_in_topbar
    endfor 
   endif 
  -%}

  <header class="header header--{{ section.settings.logo_position }} header--mobile-{{ section.settings.mobile_logo_position }} page-width{% if section.settings.menu_type_desktop == 'drawer' %} drawer-menu{% endif %}{% if section.settings.menu != blank %} header--has-menu{% endif %}{% if has_app_block %} header--has-app{% endif %}{% if social_links %} header--has-social{% endif %}{% if shop.customer_accounts_enabled %} header--has-account{% endif %}{% if localization_forms %} header--has-localizations{% endif %}{% if section.settings.search_style == 'row' %} header--has-search_row{% endif %}" data-search-style="{{ section.settings.search_style }}">
    
    {%- liquid
      if section.settings.menu != blank
        render 'header-drawer', color_sub_scheme: color_sub_scheme
      endif

      if section.settings.logo_position == 'top-center' or section.settings.logo_position == 'top-center-center' or section.settings.menu == blank
        render 'header-search', input_id: 'Search-In-Modal-1', color_sub_scheme: color_sub_scheme, search_style: section.settings.search_style, search_text: section.settings.show_search_text, search_text_reverse: section.settings.reverse_search_text
      endif
    -%}

    {%- if section.settings.logo_position != 'middle-center' -%}
      {%- if request.page_type == 'index' -%}
        <h1 class="header__heading">
      {%- endif -%}
          <a href="{{ routes.root_url }}" class="header__heading-link link link--text focus-inset">
            {%- if settings.logo != blank or settings.logo_inverted != blank -%}
              <div class="header__heading-logo-wrapper">
                {%- assign logo_alt = settings.logo.alt | default: shop.name | escape -%}
                {%- assign logo_height = settings.logo_width | divided_by: settings.logo.aspect_ratio -%}
                {% capture sizes %}(max-width: {{ settings.logo_width | times: 2 }}px) 50vw, {{ settings.logo_width }}px{% endcapture %}
                {% capture widths %}{{ settings.logo_width }}, {{ settings.logo_width | times: 1.5 | round }}, {{ settings.logo_width | times: 2 }}{% endcapture %}
                
                {% style %}
                     .header-wrapper .header__heading-logo-background{
                       background-size: {{ settings.logo_width }}px {{ logo_height }}px; 
                       width: {{ settings.logo_width }}px;
                       height: {{ logo_height }}px;
                     }
                     .scrolled-past-header [data-sticky-type="reduce-logo-size"] .header__heading-logo-background{
                      background-size: calc(0.75 * {{ settings.logo_width }}px) calc(0.75 * {{ logo_height }}px);
                      width: calc(0.75 * {{ settings.logo_width }}px);
                      height: calc(0.75 * {{ logo_height }}px);
                    }
                 {% endstyle %}

                <div class="header__heading-logo-background">
                  <span class="h2 hidden">{{ shop.name }}</span>
                </div>
                 
                {{ settings.logo | image_url: width: 600 | image_tag:
                  class: 'header__heading-logo visually-hidden motion-reduce',
                  widths: widths,
                  height: logo_height,
                  width: settings.logo_width,
                  alt: logo_alt,
                  sizes: sizes,
                  preload: true
                }}
              </div>
            {%- else -%}
              <span class="h2">{{ shop.name }}</span>
            {%- endif -%}
          </a>
      {%- if request.page_type == 'index' -%}
        </h1>
      {%- endif -%}
    {%- endif -%}
     
    {%- if section.settings.logo_position == 'top-left' or section.settings.logo_position == 'top-left-inverse' -%}
       <div class="header--part-bottom small-hide medium-hide{% if section.settings.logo_position == 'top-left-inverse' %} color-header-inverse color-inverse color-accent-2{% endif %}">
           {%- capture color_sub_scheme -%}
              color-{{ sub_color_scheme }} 
            {%- endcapture -%}
    {%- endif -%}

      {%- if section.settings.menu != blank -%}
        {% unless section.settings.menu_type_desktop == 'drawer' %} 
            <nav class="header__inline-menu">
              <ul class="list-menu list-menu--inline" role="list">
                {%- for link in section.settings.menu.links -%}
                  {% assign is_mega = false %}
                  {% assign link_name = link.title | replace: '[is_mega]', '' | downcase %}
                  
                    {% if link.title contains '[is_mega]' %}
                      {% assign is_mega = true %}
                    {% endif %}

                <li {% if is_mega %}data-name="{{ link_name }}"{% endif %}>
                    {%- if link.links != blank -%}
                      <header-menu data-openbyhover="{{ section.settings.hover_menu }}">  
                        <details id="Details-HeaderMenu-{{ forloop.index }}" class="{% if is_mega %}mega-menu{% else %}simple-menu{% endif %}">
                          <summary class="header__menu-item list-menu__item link focus-inset">
                            <span {%- unless is_mega -%}  {%- if link.child_active %} class="header__active-menu-item"{% endif %} {%- endunless -%}>{{ link.title | replace: '[is_mega]', '' | escape }}</span>
                            {% render 'icon-caret' %} 
                          </summary> 
                            
                          {%- liquid
                            if is_mega
                                assign empty_var = false
                                    for childlink in link.links
                                      if childlink.title == 'empty'
                                        assign empty_var = true
                                        endif  
                                    endfor

                              render 'header-megamenu', is_mega: is_mega, link: link, link_name: link_name, block: block, empty_var: empty_var, color_sub_scheme: color_sub_scheme
                            else
                              render 'header-simplemenu', link: link, color_sub_scheme: color_sub_scheme
                            endif
                          -%}
                        </details>
                      </header-menu> 
                        
                    {%- else -%}
                      <a href="{{ link.url }}" class="header__menu-item list-menu__item link link--text focus-inset"{% if link.current %} aria-current="page"{% endif %}>
                        <span {%- if link.current %} class="header__active-menu-item"{% endif %}>{{ link.title | replace: '[is_mega]', '' | escape }}</span>
                      </a>
                    {%- endif -%} 
                  </li>
                {%- endfor -%}
              </ul>
            </nav>
        {% else %}
          {%- if section.settings.logo_position == 'top-left' or section.settings.logo_position == 'top-left-inverse' -%}
            {%- render 'header-drawer', color_sub_scheme: color_sub_scheme -%}
          {%- endif -%}
        {% endunless %} 
      {%- endif -%}
 

    {%- unless local_in_topbar -%}
      {%- if section.settings.logo_position == 'top-left' or section.settings.logo_position == 'top-left-inverse' or section.settings.logo_position == 'top-center' and section.settings.menu_type_desktop != 'drawer'  and section.settings.enable_country_selector or section.settings.enable_language_selector -%} 
        <div class="header__localization{% if section.settings.menu_type_desktop == 'drawer' %} hidden{%- endif -%}">
                {%- if section.settings.enable_country_selector and localization.available_countries.size > 1 -%}
                  <div class="desktop-localization-wrapper">
                      {%- if section.settings.enable_country_selector or section.settings.enable_language_selector -%}
                        <noscript class="small-hide medium-hide">
                          {%- form 'localization', id: 'HeaderCountryMobileFormNoScript', class: 'localization-form' -%}
                            <div class="localization-form__select">
                              <h2 class="visually-hidden" id="HeaderCountryMobileLabelNoScript">{{ 'localization.country_label' | t }}</h2>
                              <select class="localization-selector link" name="country_code" aria-labelledby="HeaderCountryMobileLabelNoScript">
                                {%- for country in localization.available_countries -%}
                                  <option value="{{ country.iso_code }}" {%- if country.iso_code == localization.country.iso_code %} selected{% endif %}>
                                    {{ country.name }} ({{ country.currency.iso_code }} {{ country.currency.symbol }})
                                  </option>
                                {%- endfor -%}
                              </select>
                              {% render 'icon-caret' %}
                            </div>
                            <button class="button button--tertiary">{{ 'localization.update_country' | t }}</button>
                          {%- endform -%}
                        </noscript>

                        <localization-form class="small-hide medium-hide no-js-hidden">
                          {%- form 'localization', id: 'HeaderCountryForm', class: 'localization-form' -%}
                            <div>
                              <h2 class="visually-hidden" id="HeaderCountryLabel">{{ 'localization.country_label' | t }}</h2>
                              {%- render 'country-localization', localPosition: 'HeaderCountry', color_sub_scheme: color_sub_scheme -%}
                            </div>
                          {%- endform -%}
                        </localization-form>
                      {% endif %} 

                    {%- if section.settings.enable_language_selector and localization.available_languages.size > 1 -%}
                        <noscript class="small-hide medium-hide">
                          {%- form 'localization', id: 'HeaderLanguageMobileFormNoScript', class: 'localization-form' -%}
                            <div class="localization-form__select">
                              <h2 class="visually-hidden" id="HeaderLanguageMobileLabelNoScript">{{ 'localization.language_label' | t }}</h2>
                              <select class="localization-selector link" name="locale_code" aria-labelledby="HeaderLanguageMobileLabelNoScript">
                                {%- for language in localization.available_languages -%}
                                  <option value="{{ language.iso_code }}" lang="{{ language.iso_code }}" {%- if language.iso_code == localization.language.iso_code %} selected{% endif %}>
                                    {{ language.endonym_name | capitalize }}
                                  </option>
                                {%- endfor -%}
                              </select>
                              {% render 'icon-caret' %}
                            </div>
                            <button class="button button--tertiary">{{ 'localization.update_language' | t }}</button>
                          {%- endform -%}
                        </noscript>

                        <localization-form class="small-hide medium-hide no-js-hidden">
                          {%- form 'localization', id: 'HeaderLanguageForm', class: 'localization-form' -%}
                            <div>
                              <h2 class="visually-hidden" id="HeaderLanguageLabel">{{ 'localization.language_label' | t }}</h2>
                              {%- render 'language-localization', localPosition: 'HeaderLanguage', color_sub_scheme: color_sub_scheme -%}
                            </div>
                          {%- endform -%}
                        </localization-form> 
                      {%- endif -%}
                  </div>
                {%- endif -%}  
        </div>
      {%- endif -%}  
    {%- endunless -%}
   
     
    {%- if section.settings.logo_position == 'top-left' or section.settings.logo_position == 'top-left-inverse' -%}
      </div>
    {%- endif -%}

    {%- if section.settings.logo_position == 'middle-center' -%}
     {%- if request.page_type == 'index' -%}
        <h1 class="header__heading">
      {%- endif -%}
          <a href="{{ routes.root_url }}" class="header__heading-link link link--text focus-inset">
           {%- if settings.logo != blank or settings.logo_inverted != blank -%}
             <div class="header__heading-logo-wrapper">
