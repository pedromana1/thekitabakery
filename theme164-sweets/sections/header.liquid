  <script src="{{ 'cart-drawer.js' | asset_url }}" defer="defer"></script>
{%- elsif settings.cart_type == "dropdown" -%} 
  <script src="{{ 'cart-dropdown.js' | asset_url }}" defer="defer"></script>
{%- endif -%}

