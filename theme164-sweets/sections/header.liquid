<style>
  header-drawer {
    justify-self: start;
    margin-left: -1.2rem;
  }

  .menu-drawer-container {
    display: flex;
  }

  .list-menu {
    list-style: none;
    padding: 0;
    margin: 0;
  }

  .list-menu--inline {
    display: inline-flex;
    flex-wrap: wrap;
  }

  summary.list-menu__item {
    padding-right: 2.4rem;
  }

  .list-menu__item {
    display: flex;
    align-items: center;
    line-height: calc(1 + 0.3 / var(--font-body-scale));
  }

  .list-menu__item--link {
    text-decoration: none;
    padding-bottom: 1rem;
    padding-top: 1rem;
    line-height: calc(1 + 0.8 / var(--font-body-scale));
  }

  @media screen and (min-width: 750px) {
    .list-menu__item--link {
      padding-bottom: 0.5rem;
      padding-top: 0.5rem;
    }
  }
</style>

{%- style -%}

  {% unless section.settings.menu_type_desktop == 'drawer' %} 
    @media screen and (min-width: 990px) {
      header-drawer {
        display: none;
      }
    } 
  {% endunless %}

  .header-wrapper {
    --header-pt: {{ section.settings.padding_top | times: 0.5 | round: 0 }}px;
    --header-pb: {{ section.settings.padding_bottom | times: 0.5 | round: 0 }}px; 
  }

 header.header {
    padding-top: var(--header-pt);
    padding-bottom: var(--header-pb);
  }

  .section-header--in-group {
    position: sticky; /* This is for fixing a Safari z-index issue. PR #2147 */
    margin-bottom: {{ section.settings.margin_bottom | times: 0.75 | round: 0 }}px;
  }

  @media screen and (min-width: 750px) {
    .section-header--in-group {
      margin-bottom: {{ section.settings.margin_bottom }}px;
    }
  }

  {% if section.settings.show_line_separator == blank %}
   .shopify-section-header-sticky {
     box-shadow: 0 .1rem 0 0 rgba(var(--color-foreground),.05);
   } 
   {% else %}
   .shopify-section-header-sticky.scrolled-past-header:not('.shopify-section-header-hidden') .header-wrapper{
     top: -.1rem; 
   }
  {% endif %}


  @media screen and (min-width: 990px) {
    header.header {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }

    {%- if section.settings.logo_position == 'top-left' or section.settings.logo_position == 'top-left-inverse'-%}

      /* Start Inverse top left logo position */

        header.header--top-left, 
        header.header--top-left-inverse {
          padding-bottom: 0; 
          --heightElem: 5rem;
        }

        .header--part-bottom {
            display: flex;
            align-items: center;
            justify-content: space-between;
            grid-area: partbottom;
            position: relative; 
            min-height: var(--heightElem); 
        }
      
        .header--top-left,
        .header--top-left-inverse {
          grid-template-areas:
          'heading icons'
          'partbottom partbottom';
           grid-template-columns: 1fr auto;
           row-gap: {{ section.settings.padding_top }}px; 
        }

         header.header--top-left-inverse .header--part-bottom::before,
         header.header--top-left-inverse .header--part-bottom::after {
          content: '';
          position: absolute;
          bottom: 0;
          width: 100%;
          height: 100%;
          z-index: -1; 
          background-color: inherit;   
        }

         header.header--top-left-inverse .header--part-bottom::before {
          left: var(--page-x-inset);
          right: auto;
        }

        .no-js .header--top-left details[open] > .menu-drawer, 
        .no-js .header--top-left-inverse details[open] > .menu-drawer,
        .js header.header--top-left details[open].menu-opening > .menu-drawer,
        .js header.header--top-left-inverse details[open].menu-opening > .menu-drawer {
          transform: translateX(var(--page-x-inset)); 
        }

         header.header--top-left-inverse .header--part-bottom::after {
          left: auto; 
          right: var(--page-x-inset);
        }
        

      /* End Inverse top left logo position */
    {%- endif -%}

  }

  {%- if section.settings.search_style == 'row' -%}
      @media screen and (max-width: 989px) {

        .header--has-search_row {
          --mobile-input-height: 3rem;
          --search-row-mt: 0.5rem; 
          --offset-drawer: calc(var(--mobile-input-height) + var(--search-row-mt) + var(--header-pb));
        }

        .header-row-search {
          margin-top: var(--search-row-mt);
        }
      
        .header-row-search .header-search__search .search__input.field__input {
          height: var(--mobile-input-height);
        }
      
        body .header--has-search_row .menu-drawer,
        body .header--has-search_row .header__icon--menu[aria-expanded=true]:before{
          top: calc(100% - var(--offset-drawer));
          height: calc(var(--viewport-height, 100vh) - (var(--header-bottom-position, 100%)) + var(--offset-drawer));
        }

        body .header--has-search_row .header__icon--menu[aria-expanded=true]:before {
          z-index: 1;  
        }
      }  
  {%- endif -%}  
   
      .header {
        {%- if section.settings.enable_full != blank -%}
           --page-width: 100%;
           {%- else -%} 
          --page-width: {{ section.settings.page_width | divided_by: 10 }}rem; 
        {%- endif -%}   
      }

{%- endstyle -%}

<script src="{{ 'details-disclosure.js' | asset_url }}" defer="defer"></script>
<script src="{{ 'details-modal.js' | asset_url }}" defer="defer"></script>
